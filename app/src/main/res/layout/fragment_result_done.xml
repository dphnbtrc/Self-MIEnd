<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultDoneFragment">


    <TextView
        android:id="@+id/textView8"
        style="@style/HeadingText"
        android:layout_width="301dp"
        android:layout_height="147dp"
        android:gravity="center"
        android:text="Your wellness journey matters. Thank you!"
        android:textColor="@color/lapis_lazuli"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="136dp" />

    <TextView
        android:id="@+id/textView9"
        style="@style/SubheadingText"
        android:layout_width="236dp"
        android:layout_height="99dp"
        android:gravity="center"
        android:text="Would you like to try the other tests?"
        android:textColor="@color/emerald"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="339dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/customized_buttons"
        android:backgroundTint="@color/light_green"
        android:text="YES"
        android:textColor="@color/lapis_lazuli"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="496dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/customized_buttons"
        android:backgroundTint="@color/tea_green"
        android:text="EXIT"
        android:textColor="@color/lapis_lazuli"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="575dp" />

</androidx.constraintlayout.widget.ConstraintLayout>